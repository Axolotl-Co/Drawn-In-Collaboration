{
  "name": "meme teams",
  "version": "1.0.0",
  "description": "collaborative white board drawing and meme creation for teams",
  "main": "tbd",
  "scripts": {
    "start:frontend": "webpack serve --open --mode development --config webpack.config.js",
    "start:backend": "node server/server.js",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "stop": "pkill node",
    "restart": "npm run stop && npm run start",
    "build": "NODE_ENV=production webpack",
    "dev": "concurrently \"NODE_ENV=development webpack serve --open\" \"NODE_ENV=development node server/server.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.9",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.0.3",
    "sass": "^1.71.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "redux": "^5.0.1",
    "roughjs": "^4.6.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  }
}
